    <body>

      <div class="container">
        <div class="row">
          <div class="span12">
            <div class="navbar navbar-fixed-top">
              <div class="navbar-inner">
                <div class="container">
                  <a class="btn btn-navbar" data-toggle="collapse" data-target=".nav-collapse">
                    MENU
                  </a>
                  <a class="brand" href="index.asp"><img src="img/bb-logo.png"></a>
